<!doctype html>
<html lang="en">

<head>

    <%- include('../partials/head') %>

</head>

<body data-sidebar="dark">

    <%- include('../partials/modals') %>

    <!-- Begin page -->
    <div id="layout-wrapper">


        <header id="page-topbar">
            <%- include('../partials/header') %>
        </header>

        <!-- ========== Left Sidebar Start ========== -->
        <div class="vertical-menu">

            <div data-simplebar class="h-100">
                <%- include('../partials/sidebar') %>
            </div>
        </div>
        <!-- Left Sidebar End -->

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0 font-size-18">
                                    <%- menu %>
                                </h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">
                                                <%- menu %>
                                            </a></li>
                                        <li class="breadcrumb-item active">
                                            <%- submenu %>
                                        </li>
                                    </ol>
                                </div>

                            </div>
                            <div class="modal-body">
                                <div class="table-responsive export-table">
                                <table
                                    class="table table-bordered text-nowrap key-buttons border-bottom"
                                    id="datatable"
                                >
                                    <thead>
                                    <tr>
                                        <th class="text-center">action</th>
                                        <th class="text-center">app</th>
                                        <th class="text-center">appid</th>
                                        <th class="text-center">apprisk</th>
                                        <th class="text-center">crlevel</th>
                                        <th class="text-center">datetime</th>
                                        <th class="text-center">devname</th>
                                        <th class="text-center">document_id</th>
                                        <th class="text-center">dstport</th>
                                        <th class="text-center">dstip</th>
                                        <th class="text-center">eventype</th>
                                        <th class="text-center">host</th>
                                        <th class="text-center">level</th>
                                        <th class="text-center">message</th>
                                        <th class="text-center">path</th>
                                        <th class="text-center">policyid</th>
                                        <th class="text-center">service</th>
                                        <th class="text-center">sessionid</th>
                                        <th class="text-center">srccountry</th>
                                        <th class="text-center">time</th>
                                        <th class="text-center">type</th>
                                </tr>
                                </thead>
                            </table>
                            </div>
                            </div>
                        </div>
                    </div>
                    <!-- end page title -->

                    <div class="row">
                    </div>



                    <footer class="footer">
                        <%- include('../partials/footer') %>
                    </footer>
                </div>
                <!-- end main content-->

            </div>
            <!-- END layout-wrapper -->



            <!-- Right bar overlay-->
            <div class="rightbar-overlay"></div>

            <%- include('../partials/js') %>

                <script src="https://cdn.amcharts.com/lib/4/core.js"></script>
                <script src="https://cdn.amcharts.com/lib/4/charts.js"></script>
                <script src="https://cdn.amcharts.com/lib/4/themes/animated.js"></script>

                <script src="assets/js/pages/dashboard.init.js"></script>

                <script type="text/javascript">
                    toastr.info('Visited Table View!', 'Notification', {
                        "positionClass": "toast-top-right", "showDuration": "300",
                        "hideDuration": "1000",
                        "timeOut": "1000",
                        "extendedTimeOut": "1000",
                    })
                </script>
</body>

</html>