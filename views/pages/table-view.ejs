<!doctype html>
<html lang="en">

<head>
    
    <%- include('../partials/head') %>
    
</head>

<body data-sidebar="dark">
    
    <%- include('../partials/modals') %>
    
    <!-- Begin page -->
    <div id="layout-wrapper">
        
        
        <header id="page-topbar">
            <%- include('../partials/header') %>
        </header>
        
        <!-- ========== Left Sidebar Start ========== -->
        <div class="vertical-menu">
            
            <div data-simplebar class="h-100">
                <%- include('../partials/sidebar') %>
            </div>
        </div>
        <!-- Left Sidebar End -->
        
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">
            
            <div class="page-content">
                <div class="container-fluid">
                    
                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                                <h4 class="mb-sm-0 font-size-18">
                                    <%- menu %>
                                </h4>
                                
                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">
                                            <%- menu %>
                                        </a></li>
                                        <li class="breadcrumb-item active">
                                            <%- submenu %>
                                        </li>
                                    </ol>
                                </div>
                                
                            </div>
                            
                            <div class="row">
                                <div class="col-12">
                                    <div class="card">
                                        <div class="card-body">
                                            <div class="table-responsive export-table">
                                                <table
                                                class="table table-bordered text-nowrap key-buttons border-bottom"
                                                id="table-view">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center">DATE</th>
                                                        <th class="text-center">APP RISK</th>
                                                        <th class="text-center">SRCCOUNTRY</th>
                                                        <th class="text-center">MESSAGE</th>
                                                        <th class="text-center">RATE METHOD</th>
                                                        <th class="text-center">PATH</th>
                                                        <th class="text-center">HOSTNAME</th>
                                                        <th class="text-center">SRC IP</th>
                                                        <th class="text-center">DEV NAME</th>
                                                        <th class="text-center">LEVEL</th>
                                                        <th class="text-center">DIRECTION</th>
                                                        <th class="text-center">SERVICE</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                            <div class="text-center">
                                                <div class="lds-facebook" id="lds-facebook"><div></div><div></div><div></div></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div> <!-- end col -->
                        </div> <!-- end row -->
                    </div>
                </div>
                <!-- end page title -->
                
                <div class="row">
                </div>
                
                
                
                <footer class="footer">
                    <%- include('../partials/footer') %>
                </footer>
            </div>
            <!-- end main content-->
            
        </div>
        <!-- END layout-wrapper -->
        
        
        
        <!-- Right bar overlay-->
        <div class="rightbar-overlay"></div>
        
        <%- include('../partials/js') %>
        <script src="assets/js/pages/dashboard.init.js"></script>
        <script type="text/javascript">
            toastr.info('Visited Table View!', 'Notification', {
                "positionClass": "toast-top-right", "showDuration": "300",
                "hideDuration": "1000",
                "timeOut": "1000",
                "extendedTimeOut": "1000",
            })
        </script>
    </body>
    
    </html>
